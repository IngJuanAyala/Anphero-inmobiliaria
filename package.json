    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "sass": "^1.58.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
